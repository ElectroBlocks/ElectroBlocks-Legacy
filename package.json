{
  "name": "ElectroBlocks",
  "productName": "ElectroBlocks A Blockly Based Arduino IDE",
  "email": "Noah Glaser",
  "version": "2.0.8",
  "description": "An ide for developing arduino code with blockly.",
  "main": "app.js",
  "author": "Noah Glaser",
  "repository": "git@github.com:phptuts/ElectroBlocks.git",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "dist-win": "build --win",
    "publish": "build -p always",
    "dist-mac": "build --mac",
    "publish-mac": "build  --mac -p always",
    "publish-win": "build  --win -p always",
    "dist-linux": "build --linux",
    "publish-linux": "build  --linux -p always",
    "postinstall": "electron-builder install-app-deps",
    "test": "ts-node node_modules/jasmine/bin/jasmine \"**/*.spec.ts\""
  },
  "build": {
    "appId": "electroblocks.codingwithnoah.org",
    "productName": "ElectroBlocks",
    "copyright": "Copyright Â© 2019",
    "win": {
      "publisherName": "Noah Glaser",
      "certificateSubjectName": "Noah Glaser",
      "target": "nsis",
      "icon": "build/win-icon.ico"
    },
    "mac": {
      "category": "public.arduino.tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "maintainer": "Noah Glaser"
    },
    "dmg": {
      "backgroundColor": "#faf9ef",
      "publish": {
        "provider": "s3",
        "bucket": "oaklandcodeschool",
        "path": "osx"
      },
      "window": {
        "width": 300,
        "height": 250,
        "x": 50,
        "y": 0
      },
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/electron": "^1.6.10",
    "@types/uuid": "^3.4.4",
    "avrgirl-arduino": "^3.0.0",
    "axios": "0.18.0",
    "bluebird": "^3.5.5",
    "displacejs": "^1.2.4",
    "electron-log": "^2.2.17",
    "electron-online": "^1.0.0",
    "electron-prompt": "0.5.0",
    "electron-publisher-s3": "^20.9.0",
    "electron-updater": "^4.0.6",
    "johnny-five": "^1.0.0",
    "lodash": "^4.17.11",
    "rxjs": "^5.5.8",
    "serialport": "^7.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^12.0.8",
    "@types/semver": "^6.0.0",
    "@types/serialport": "^7.0.3",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "jasmine": "^3.3.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
}
